doctype
html
  head
    title #{title}
    link(rel='stylesheet', href='/stylesheets/style.css')
  body
    header
      h1 Commits
    .container
      .main-content
        block content
      .sidebar
        block sidebar
    footer
      .p commits
      each item in commits
        li= item.git
        li= item.url
        li= item.message
        li= item.author
